<?xml version="1.0" encoding="UTF-8"?>
<dataSchema>
  <RelationshipElement name="TransectSUI">
    <description>Defines relationship between Transect and Survey Unit</description>
  </RelationshipElement>
  <ArchaeologicalElement name="POI">
    <description></description>
    <property name="POI ID" type="freetext" isIdentifier="true">
      <description/>
    </property>
    <property name="Latitude" type="float">
      <description/>
    </property>
    <property name="Longitude" type="float">
      <description/>
    </property>
    <property name="Northing" type="float">
      <description/>
    </property>
    <property name="Easting" type="float">
      <description/>
    </property>
    <property name="Object" type="enum">
      <description/>
      <lookup>
        <term> {Core}
          <description/>
        </term>
        <term> {Cobble}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Weight" type="float">
      <description/>
    </property>
    <property name="Maximum length" type="float">
      <description/>
    </property>
    <property name="Maximum width" type="float">
      <description/>
    </property>
    <property name="Maximum thickness" type="float">
      <description/>
    </property>
    <property name="Raw material" type="enum">
      <description/>
      <lookup>
        <term> {No_Observation}
          <description/>
        </term>
        <term> {Quartz}
          <description/>
        </term>
        <term> {Quartzite}
          <description/>
        </term>
        <term> {Chert}
          <description/>
        </term>
        <term> {Silcrete}
          <description/>
        </term>
        <term> {Other_annotation}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Crystal Size" type="enum">
      <description>Estimated by using a sand gauge (©1984 W.F. McCullough)”</description>
      <lookup>
        <term> {No_Observation}
          <description/>
        </term>
        <term> {Fine}
          <description/>
        </term>
        <term> {Medium}
          <description/>
        </term>
        <term> {Coarse}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Abundance of Flaws" type="enum">
      <description>Percentage of impurities mottles, large coarse fragments, and natural fissures measured on two spots on the inside of the cobble in a 2 x 2 cm square, using Munsell Charts for Estimating Proportions of Mottles and Coarse Fragments</description>
      <lookup>
        <term> {10_percent}
          <description/>
        </term>
        <term> {20_percent}
          <description/>
        </term>
        <term> {30_percent}
          <description/>
        </term>
        <term> {40_percent}
          <description/>
        </term>
        <term> {50_percent}
          <description/>
        </term>
        <term> {60_percent}
          <description/>
        </term>
        <term> {70_percent}
          <description/>
        </term>
        <term> {80_percent}
          <description/>
        </term>
        <term> {90_percent}
          <description/>
        </term>
        <term> {100_percent}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Photo File Numbers" type="freetext">
      <description/>
    </property>
    <property name="Other Comments" type="freetext">
      <description>List the camera ID of the pictures you have taken of this transect</description>
    </property>
    <property name="Angularity" type="enum">
      <description/>
      <lookup>
        <term> {No_Observation}
          <description/>
        </term>
        <term> {Rounded}
          <description/>
        </term>
        <term> {Sub_rounded}
          <description/>
        </term>
        <term> {Angular}
          <description/>
        </term>
        <term> {Sub_angular}
          <description/>
        </term>
        <term> {Other_annotation}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Completeness" type="enum">
      <description/>
      <lookup>
        <term> {Complete}
          <description/>
        </term>
        <term> {Broken}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Core Weathering stage" type="enum">
      <description/>
      <lookup>
        <term> {No_edge_rounding}
          <description/>
        </term>
        <term> {Edge_rounding_visible}
          <description/>
        </term>
        <term> {Edge_rounding_visible_to_eye}
          <description/>
        </term>
        <term> {Edge_rounding_obscured}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Outer Surface (Cortex) on Whole Core" type="enum">
      <description/>
      <lookup>
        <term> {None}
          <description/>
        </term>
        <term> {Less_than_25}
          <description/>
        </term>
        <term> {25-50}
          <description/>
        </term>
        <term> {50-75}
          <description/>
        </term>
        <term> {Greater_than_75}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Outer Surface (Cortex) on Upper Hemisphere" type="enum">
      <description>Face with the lowest amount of cortex</description>
      <lookup>
        <term> {None}
          <description/>
        </term>
        <term> {Less_than_25}
          <description/>
        </term>
        <term> {25-50}
          <description/>
        </term>
        <term> {50-75}
          <description/>
        </term>
        <term> {Greater_than_75}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Outer Surface (Cortex) on Lower Hemisphere" type="enum">
      <description>Face with the higest amount of cortex</description>
      <lookup>
        <term> {None}
          <description/>
        </term>
        <term> {Less_than_25}
          <description/>
        </term>
        <term> {25-50}
          <description/>
        </term>
        <term> {50-75}
          <description/>
        </term>
        <term> {Greater_than_75}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Number of Flake Scars" type="float">
      <description>All scars ≥ 10 mm in length and width</description>
    </property>
    <property name="Number of Platforms" type="float">
      <description/>
    </property>
    <property name="Flaking on Core Perimeter" type="enum">
      <description/>
      <lookup>
        <term> {Not_applicable}
          <description/>
        </term>
        <term> {Less_than_20}
          <description/>
        </term>
        <term> {20-40}
          <description/>
        </term>
        <term> {40-60}
          <description/>
        </term>
        <term> {60-80}
          <description/>
        </term>
        <term> {Greater_than_80}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Core Typology" type="enum">
      <description/>
      <lookup>
        <term> {No_Observation}
          <description/>
        </term>
        <term> {Levallois}
          <description/>
        </term>
        <term> {Disc}
          <description/>
        </term>
        <term> {Discoidal}
          <description/>
        </term>
        <term> {Single_platformed_Unifacial}
          <description/>
        </term>
        <term> {Single_Platformed_Bifacial}
          <description/>
        </term>
        <term> {Double_Platformed_Opposed}
          <description/>
        </term>
        <term> {Double_Platformed_at_Right_Angles}
          <description/>
        </term>
        <term> {Multiple_Platformed}
          <description/>
        </term>
        <term> {Shapeless/Miscellaneous}
          <description/>
        </term>
        <term> {Blade}
          <description/>
        </term>
        <term> {Bipolar}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Photo" type="file" file="true" thumbnail="true">
      <description/>
    </property>
  </ArchaeologicalElement>


  <ArchaeologicalElement name="Tracklog">
    <description></description>
    <property name="Timestamp" type="string" isIdentifier="true">
      <description>Description TBD</description>
    </property>
   <property name="TracklogTeam" type="string" isIdentifier="true">
      <description>Description TBD</description>
    </property>

    <property name="Latitude" type="float">
      <description>Description TBD</description>
    </property>
    <property name="Longitude" type="float">
      <description>Description TBD</description>
    </property>
    <property name="Heading" type="float">
      <description>Description TBD</description>
    </property>
    <property name="Accuracy" type="float">
      <description>Description TBD</description>
    </property>
  </ArchaeologicalElement>
  <ArchaeologicalElement name="Transect">
    <description></description>
    <property name="Timestamp" type="string">
      <description>Description TBD</description>
    </property>
   <property name="TracklogTeam" type="string" isIdentifier="true">
      <description>Description TBD</description>
    </property>    
    <property name="SUID" type="freetext" isIdentifier="true">
      <description>Description TBD</description>
    </property>
    <property name="State" type="freetext" isIdentifier="true">
      <description>Description TBD</description>
    </property>
    <property name="Width" type="freetext">
      <description>Description TBD</description>
    </property>
    <property name="Latitude" type="float">
      <description>Description TBD</description>
    </property>
    <property name="Longitude" type="float">
      <description>Description TBD</description>
    </property>
    <property name="Heading" type="float">
      <description>Description TBD</description>
    </property>
    <property name="Accuracy" type="float">
      <description>Description TBD</description>
    </property>

  </ArchaeologicalElement>

  <ArchaeologicalElement name="Survey Unit" type="freetext">
    <description></description>
    <property name="Survey Unit ID" type="freetext" isIdentifier="true">
      <description>Unique identifier for this transect</description>
    </property>
    <property name="Total Transect Width" type="freetext">
      <description>Total Transect Width</description>
    </property>
    <property name="Number of Walkers" type="float">
      <description>Description TBD</description>
    </property>
    <property name="Participants" type="float">
      <description>Description TBD</description>
    </property>
    <property name="Photo file numbers" type="freetext">
      <description>List the camera ID of the pictures you have taken of this transect</description>
    </property>
    <property name="Landform" type="enum">
      <description>Select the type of landform</description>
      <lookup>
        <term> {No_Observation}
          <description/>
        </term>
        <term> {Range_top}
          <description/>
        </term>
        <term> {Upper_slope}
          <description/>
        </term>
        <term> {Middle_slope}
          <description/>
        </term>
        <term> {Lower_slope}
          <description/>
        </term>
        <term> {Flat_land}
          <description/>
        </term>
        <term> {Hill}
          <description/>
        </term>
        <term> {River_flat}
          <description/>
        </term>
        <term> {Creek_flat}
          <description/>
        </term>
        <term> {Long_gradual_slope}
          <description/>
        </term>
        <term> {Short_steep_slope}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Remarks on landform" type="freetext">
      <description>Comments such as: degrees of slope angles measured/estimated with a clinometer, incised stream valleys, size of landform, which geological characteristics are present on the geological map, e.g. Chitimwe, Chiwondo, Dinosaur Beds, etc</description>
    </property>
    <property name="Average percentage of visibility of surface of the landform area" type="enum">
      <description>Visibility determined and recorded to the nearest 10 %, taking into account grass cover, leaf litter, etc.</description>
      <lookup>
        <term> {10_percent}
          <description/>
        </term>
        <term> {20_percent}
          <description/>
        </term>
        <term> {30_percent}
          <description/>
        </term>
        <term> {40_percent}
          <description/>
        </term>
        <term> {50_percent}
          <description/>
        </term>
        <term> {60_percent}
          <description/>
        </term>
        <term> {70_percent}
          <description/>
        </term>
        <term> {80_percent}
          <description/>
        </term>
        <term> {90_percent}
          <description/>
        </term>
        <term> {100_percent}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Remarks on visibility" type="freetext">
      <description>Comments such as: open patches, field, short or tall grass, trees, buildings, water surface, etc</description>
    </property>
    <property name="Average percentage of exposure of artefacts" type="enum">
      <description>Exposure estimated to the nearest 10 % as the mean percentage of the surface area of the survey where erosion was sufficient to reveal objects on the surface of the ground.</description>
      <lookup>
        <term> {10_percent}
          <description/>
        </term>
        <term> {20_percent}
          <description/>
        </term>
        <term> {30_percent}
          <description/>
        </term>
        <term> {40_percent}
          <description/>
        </term>
        <term> {50_percent}
          <description/>
        </term>
        <term> {60_percent}
          <description/>
        </term>
        <term> {70_percent}
          <description/>
        </term>
        <term> {80_percent}
          <description/>
        </term>
        <term> {90_percent}
          <description/>
        </term>
        <term> {100_percent}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Remarks on exposure" type="freetext">
      <description>Comments such as: erosion and other things that can cause erosion; agricultural activity, grazing, road disturbance, etc.</description>
    </property>
    <property name="Basic geomorphic summary" type="enum">
      <description>Comments such as: erosion and other things that can cause erosion; agricultural activity, grazing, road disturbance, etc.</description>
      <lookup>
        <term> {No_Observation}
          <description/>
        </term>
        <term> {Bedrock}
          <description/>
        </term>
        <term> {Lag_on_bedrock}
          <description/>
        </term>
        <term> {Lag_on_sediments}
          <description/>
        </term>
        <term> {Colluvium}
          <description/>
        </term>
        <term> {Alluvial_fan}
          <description/>
        </term>
        <term> {Floodplain_alluvium}
          <description/>
        </term>
        <term> {Fluvially_incised_sediments_strath_terraces}
          <description/>
        </term>
        <term> {Other}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Cobble distribution" type="enum">
      <description/>
      <lookup>
        <term> {No_Observation}
          <description/>
        </term>
        <term> {Not_present}
          <description/>
        </term>
        <term> {Evenly_scattered}
          <description/>
        </term>
        <term> {Quite_evenly}
          <description/>
        </term>
        <term> {Aggregated}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Estimated number of cobbles per m2" type="float">
      <description>Estimate the number of cobbles per square m</description>
    </property>
    <property name="Other remarks on cobbles" type="freetext">
      <description></description>
    </property>
    <property name="Sediment size" type="enum">
      <description>Select all sediment sizes that apply</description>
      <lookup>
        <term> {Fine}
          <description/>
        </term>
        <term> {Coarse}
          <description/>
        </term>
        <term> {Small_Clasts}
          <description/>
        </term>
        <term> {Large_Clasts}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Sediment thickness" type="enum">
      <description/>
      <lookup>
        <term> {Less_than_10cm}
          <description/>
        </term>
        <term> {Greater_than_10cm}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Other remarks on transect" type="freetext">
      <description>Any other comments on the transect recording</description>
    </property>
    <property name="Team" type="file">
      <description>Description TBD</description>
      <lookup>
       <term>A<description/></term>
       <term>B<description/></term>
       <term>C<description/></term>
      </lookup>
    </property>
    <property name="Transect Photo" type="file" file="true" thumbnail="true">
      <description/>
    </property>
  </ArchaeologicalElement>

  <ArchaeologicalElement name="Artefact">
    <description></description>
    <property name="Artefact ID" type="freetext" isIdentifier="true">
      <description/>
    </property>
    <property name="X" type="float">
      <description/>
    </property>
    <property name="Y" type="float">
      <description/>
    </property>
    <property name="Z" type="float">
      <description/>
    </property>
    <property name="Technological Component" type="enum">
      <description/>
      <lookup>
        <term> {Flake}
          <description/>
        </term>
        <term> {Core}
          <description/>
        </term>
        <term> {Shatter_Flake_Piece}
          <description/>
        </term>
        <term> {Other}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Weight" type="float">
      <description/>
    </property>
    <property name="Maximum length" type="float">
      <description>For flakes, this must be measured longitudinally, e.g from the platform to the termination</description>
    </property>
    <property name="Maximum width" type="float">
      <description>For flakes, this must be measured transversely, e.g from one lateral margin of the flake to the other</description>
    </property>
    <property name="Maximum thickness" type="float">
      <description/>
    </property>
    <property name="Thickness Measurement site" type="enum">
      <description/>
      <lookup>
        <term> {Bulb}
          <description/>
        </term>
        <term> {Other_describe}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Maximum Dimension" type="float">
      <description>The maximum dimension across the object</description>
    </property>
    <property name="Raw material" type="enum">
      <description/>
      <lookup>
        <term> {No_Observation}
          <description/>
        </term>
        <term> {Quartz}
          <description/>
        </term>
        <term> {Quartzite}
          <description/>
        </term>
        <term> {Chert}
          <description/>
        </term>
        <term> {Silcrete}
          <description/>
        </term>
        <term> {Other_annotation}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Artefact Weathering stage" type="enum">
      <description/>
      <lookup>
        <term> 0
          <description/>
        </term>
        <term> 1
          <description/>
        </term>
        <term> 2
          <description/>
        </term>
        <term> 3
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Longitudinal Portion" type="enum">
      <description/>
      <lookup>
        <term> {No_Observation}
          <description/>
        </term>
        <term> {Complete}
          <description/>
        </term>
        <term> {Proximal}
          <description/>
        </term>
        <term> {Medial}
          <description/>
        </term>
        <term> {Distal}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Transverse Portion" type="enum">
      <description/>
      <lookup>
        <term> {No_Observation}
          <description/>
        </term>
        <term> {Complete}
          <description/>
        </term>
        <term> {Split_Left}
          <description/>
        </term>
        <term> {Middle}
          <description/>
        </term>
        <term> {Split_Right}
          <description/>
        </term>
        <term> {Margin}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Platform Type" type="enum">
      <description/>
      <lookup>
        <term> {No_Observation}
          <description/>
        </term>
        <term> {Cortical}
          <description/>
        </term>
        <term> {Plain}
          <description/>
        </term>
        <term> {Dihedral}
          <description/>
        </term>
        <term> {Polyhedral}
          <description/>
        </term>
        <term> {Faceted}
          <description/>
        </term>
        <term> {Crushed_Shattered}
          <description/>
        </term>
        <term> {Point}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Platform Cortex" type="enum">
      <description/>
      <lookup>
        <term> {No_Observation}
          <description/>
        </term>
        <term> {No_Cortex}
          <description/>
        </term>
        <term> {Partial_Platform_Cortex}
          <description/>
        </term>
        <term> {Complete_Platform_Cortex}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Termination" type="enum">
      <description/>
      <lookup>
        <term> {No_Observation}
          <description/>
        </term>
        <term> {Feather}
          <description/>
        </term>
        <term> {Overshot}
          <description/>
        </term>
        <term> {Hinge}
          <description/>
        </term>
        <term> {Step}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Percentage Cortical Coverage" type="enum">
      <description>Percentage of the dorsal and platform surfaces combined that is covered in cortex.</description>
      <lookup>
        <term> {No_Observation}
          <description/>
        </term>
        <term> {0-24}
          <description/>
        </term>
        <term> {25-49}
          <description/>
        </term>
        <term> {50-74}
          <description/>
        </term>
        <term> {75-99}
          <description/>
        </term>
        <term> {100}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Flake Typology" type="enum">
      <description/>
      <lookup>
        <term> {None}
          <description/>
        </term>
        <term> {Levallois}
          <description/>
        </term>
        <term> {Blade}
          <description/>
        </term>
        <term> {Point}
          <description/>
        </term>
        <term> {Backed}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Scar Orientation" type="enum">
      <description/>
      <lookup>
        <term> {No_Observation}
          <description/>
        </term>
        <term> {Indeterminate}
          <description/>
        </term>
        <term> {Levallois}
          <description/>
        </term>
        <term> {Unidirectional}
          <description/>
        </term>
        <term> {Bidirectional_Opposed}
          <description/>
        </term>
        <term> {Bidirectional_Orthogonal}
          <description/>
        </term>
        <term> {Centripetal}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Number of Dorsal Scars" type="float">
      <description>Only count scars >15 mm in the maximum dimension</description>
    </property>
    <property name="Retouched" type="enum">
      <description>Describe the retouch, e.g. invasive, non-invasive, steep, shallow, etc.</description>
      <lookup>
        <term> {Yes}
          <description/>
        </term>
        <term> {No}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Flake Comments" type="freetxt">
      <description>Description TBD</description>
    </property>
    <property name="Completeness" type="enum">
      <description/>
      <lookup>
        <term> {Complete}
          <description/>
        </term>
        <term> {Broken}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Core Weathering stage" type="enum">
      <description/>
      <lookup>
        <term> {No_Observation}
          <description/>
        </term>
        <term> {No_edge_rounding}
          <description/>
        </term>
        <term> {Edge_rounding_visible}
          <description/>
        </term>
        <term> {Edge_rounding_visible_to_eye}
