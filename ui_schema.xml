<?xml version="1.0" encoding="utf-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa">


  <h:head>
    <h:title>Tracklog Demonstration</h:title>
    <!--model lists user interface tabgroups, tabs and content of tabs-->
    <model>
      <instance>
        <faims id="tracklog">
          <style>
            <orientation>
              <orientation/>
            </orientation>
            <even>
              <layout_weight/>
            </even>
            <leftBigger>
              <layout_weight/>
            </leftBigger>
          </style>
          <user>
            <usertab>
              <users/>
              <Team/>
              <login/>
            </usertab>
          </user>
          <control>
            <Control>
              <internalExternalGPS/>
              <container1>
                <child1>
                  <refreshText/>
                </child1>
                <child2>
                  <refreshGPSButton/>
                </child2>
              </container1>
              <container2>
                <child1>
                  <trackdistText/>
                </child1>
                <child2>
                  <trackdistButton/>
                </child2>
              </container2>
              <container3>
                <child1>
                  <tracktimeText/>
                </child1>
                <child2>
                  <tracktimeButton/>
                </child2>
              </container3>
              <trackOffButton/>
              <trackStatus/>
              <SyncOn/>
              <SyncOff/>
            </Control>
            <Start>
              <container1>
                <child1>
                  <TeamMember/>
                </child1>
                <child2>
                  <Walker/>
                </child2>
              </container1>
              <newSurveyUnit/>
              <currentSurveyUnit/>
              <finishSU/>
              <container2>
                <child1>
                  <newPOI/>
                </child1>
                <child2>
                  <newArtefact/>
                </child2>
              </container2>
              <listOfLastText/>
              <archentsRadio/>
              <listOfLast/>
            </Start>
            <Map>
              <map/>
              <centre/>
            </Map>
          </control>
          <Survey_Unit>
            <Survey_Unit>
              <Survey_Unit_ID/>
              <Total_Transect_Width/>
              <Photo_file_numbers/>
              <Landform/>
              <Remarks_on_landform/>
              <Average_percentage_of_visibility_of_surface_of_the_landform_area/>
              <Remarks_on_visibility/>
              <Average_percentage_of_exposure_of_artefacts/>
              <Remarks_on_exposure/>
              <Basic_geomorphic_summary/>
              <Cobble_distribution/>
              <Estimated_number_of_cobbles_per_m2/>
              <Other_remarks_on_cobbles/>
              <Sediment_size/>
              <Other_remarks_on_transect/>
              <Photo/>
              <Attach_Photo/>
              <Update_and_Return/>
              <Delete/>
              <trackStatus/>
            </Survey_Unit>
          </Survey_Unit>
          <POI>
            <POI>
              <POI_ID/>
              <container1>
                <child1>
                  <Latitude/>
                </child1>
                <child2>
                  <Longitude/>
                </child2>
              </container1>
              <Take_GPS/>
              <Object/>
              <Weight/>
              <container2>
                <child1>
                  <Maximum_length/>
                </child1>
                <child2>
                  <Maximum_width/>
                </child2>
                <child3>
                  <Maximum_thickness/>
                </child3>
              </container2>
              <Raw_material/>
              <Crystal_Size/>
              <Abundance_of_Flaws/>
              <Photo_File_Numbers/>
              <Other_Comments/>
              <Photo/>
              <Attach_Photo/>
              <Update_and_Next/>
              <Delete/>
            </POI>
            <Cobble>
              <POI_ID/>
              <Angularity/>
              <Update_and_Return/>
              <Delete/>
            </Cobble>
            <Core>
              <POI_ID/>
              <Completeness/>
              <Core_Weathering_stage/>
              <Outer_Surface_on_Whole_Core/>
              <Outer_Surface_on_Upper_Hemisphere/>
              <Outer_Surface_on_Lower_Hemisphere/>
              <Number_of_Flake_Scars/>
              <Number_of_Platforms/>
              <Flaking_on_Core_Perimeter/>
              <Update_and_Return/>
              <Delete/>
            </Core>
          </POI>
          <Artefact>
            <Artefact>
              <Artefact_ID/>
              <Technological_Component/>
              <Weight/>
              <container1>
                <child1>
                  <Maximum_length/>
                </child1>
                <child2>
                  <Maximum_width/>
                </child2>
                <child3>
                  <Maximum_thickness/>
                </child3>
              </container1>
              <Thickness_Measurement_site/>
              <Maximum_Dimension/>
              <Raw_material/>
              <Artefact_Weathering_stage/>
              <Update_and_Next/>
              <Delete/>
            </Artefact>
            <Flake>
              <Artefact_ID/>
              <Longitudinal_Portion/>
              <Transverse_Portion/>
              <Platform_Type/>
              <Platform_Cortex/>
              <Termination/>
              <Percentage_Cortical_Coverage/>
              <Flake_Typology/>
              <Scar_Orientation/>
              <Number_of_Dorsal_Scars/>
              <Retouched/>
              <Flake_Comments/>
              <Photo/>
              <Attach_Photo/>
              <Update_and_Return/>
              <Delete/>
            </Flake>
            <Core>
              <Artefact_ID/>
              <Completeness/>
              <Core_Weathering_stage/>
              <Outer_Surface_on_Whole_Core/>
              <Outer_Surface_on_Upper_Hemisphere/>
              <Outer_Surface_on_Lower_Hemisphere/>
              <Number_of_Flake_Scars/>
              <Number_of_Platforms/>
              <Flaking_on_Core_Perimeter/>
              <Typology/>
              <Photo/>
              <Attach_Photo/>
              <Update_and_Return/>
              <Delete/>
            </Core>
            <Shatter_Flakes>
              <Artefact_ID/>
              <Percentage_Cortical_Coverage/>
              <Photo/>
              <Attach_Photo/>
              <Update_and_Return/>
              <Delete/>
            </Shatter_Flakes>
            <Other>
              <Artefact_ID/>
              <Other_Type/>
              <Photo/>
              <Attach_Photo/>
              <Update_and_Return/>
              <Delete/>
            </Other>
          </Artefact>
        </faims>
      </instance>
      <bind nodeset="/faims/Survey_Unit/Survey_Unit/Estimated_number_of_cobbles_per_m2" type="decimal"/>
      <bind nodeset="/faims/Survey_Unit/Survey_Unit/Total_Transect_Width" type="decimal"/>      
      <bind nodeset="/faims/POI/POI/container1/child1/Latitude" type="decimal"/>
      <bind nodeset="/faims/POI/POI/container1/child2/Longitude" type="decimal"/>
      <bind nodeset="/faims/POI/POI/Weight" type="decimal"/>
      <bind nodeset="/faims/POI/POI/container2/child1/Maximum_length" type="decimal"/>
      <bind nodeset="/faims/POI/POI/container2/child2/Maximum_width" type="decimal"/>
      <bind nodeset="/faims/POI/POI/container2/child3/Maximum_thickness" type="decimal"/>
      <bind nodeset="/faims/POI/Core/Number_of_Flake_Scars" type="decimal"/>
      <bind nodeset="/faims/POI/Core/Number_of_Platforms" type="decimal"/>
      <bind nodeset="/faims/control/Start/Walker" type="decimal"/>
      <bind nodeset="/faims/Artefact/Artefact/Weight" type="decimal"/>
      <bind nodeset="/faims/Artefact/Artefact/container1/child1/Maximum_length" type="decimal"/>
      <bind nodeset="/faims/Artefact/Artefact/container1/child2/Maximum_width" type="decimal"/>
      <bind nodeset="/faims/Artefact/Artefact/container1/child3/Maximum_thickness" type="decimal"/>
      <bind nodeset="/faims/Artefact/Artefact/Maximum_Dimension" type="decimal"/>
      <bind nodeset="/faims/Artefact/Flake/Number_of_Dorsal_Scars" type="decimal"/>
      <bind nodeset="/faims/Artefact/Core/Number_of_Flake_Scars" type="decimal"/>
      <bind nodeset="/faims/Artefact/Core/Number_of_Platforms" type="decimal"/>
    </model>
  </h:head>
  <h:body>
    <!--Describes controls for each tabgroup and its nested tabs-->
    <group ref="style">
      <label/>
      <group ref="orientation">
        <label/>
        <input ref="orientation">
          <label>horizontal</label>
        </input>
      </group>
      <group ref="even">
        <label/>
        <input ref="layout_weight">
          <label>1</label>
        </input>
      </group>
      <group ref="leftBigger">
        <label/>
        <input ref="layout_weight">
          <label>2</label>
        </input>
      </group>
    </group>
    <group ref="user">
      <label>User List</label>
      <group ref="usertab">
        <label>User List</label>
        <select1 ref="users">
          <label>Users:</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <select1 appearance="full" ref="Team">
          <label>Team</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <trigger ref="login">
          <label>Login</label>
        </trigger>
      </group>
    </group>
    <group ref="control" faims_scrollable="false">
      <label>Control</label>
      <group ref="Control">
        <label>Control</label>
        <select1 ref="internalExternalGPS" appearance="full">
          <label>Choose which GPS to connect to:</label>
          <item>
            <label>Internal</label>
            <value>I</value>
          </item>
          <item>
            <label>External</label>
            <value>E</value>
          </item>
        </select1>
        <group faims_style="orientation" ref="container1">
          <label/>
          <group faims_style="even" ref="child1">
            <label/>
            <input faims_certainty="false" faims_read_only="false" ref="refreshText">
              <label>GPS Refresh Rate (s)</label>
            </input>
          </group>
          <group faims_style="even" ref="child2">
            <label/>
            <trigger ref="refreshGPSButton">
              <label>Start GPS</label>
            </trigger>
          </group>
        </group>
        <group faims_style="orientation" ref="container2">
          <label/>
          <group faims_style="even" ref="child1">
            <label/>
            <input faims_certainty="false" faims_read_only="false" ref="trackdistText">
              <label>Tracklog Distance Interval (m)</label>
            </input>
          </group>
          <group faims_style="even" ref="child2">
            <label/>
            <trigger ref="trackdistButton">
              <label>Set Tracklog Distance Interval</label>
            </trigger>
          </group>
        </group>
        <group faims_style="orientation" ref="container3">
          <label/>
          <group faims_style="even" ref="child1">
            <label/>
            <input faims_certainty="false" faims_read_only="false" ref="tracktimeText">
              <label>Tracklog Time Interval (s)</label>
            </input>
          </group>
          <group faims_style="even" ref="child2">
            <label/>
            <trigger ref="tracktimeButton">
              <label>Set Tracklog Time Interval</label>
            </trigger>
          </group>
        </group>
        <trigger ref="trackOffButton">
          <label>Tracklog Off</label>
        </trigger>
        <input faims_certainty="false" faims_read_only="true" ref="trackStatus">
          <label>Track Log Status</label>
        </input>
        <trigger ref="SyncOn">
          <label>Sync On</label>
        </trigger>
        <trigger ref="SyncOff">
          <label>Sync Off</label>
        </trigger>
      </group>

      <group ref="Start" faims_scrollable="false">
        <label>Start</label>
        <group faims_style="orientation" ref="container1">
          <label/>
          <group faims_style="even" ref="child1">
            <label/>
            <input faims_certainty="false" faims_read_only="false" ref="TeamMember">
              <label>Team Members</label>
            </input>
          </group>
          <group faims_style="even" ref="child2">
            <label/>
            <input faims_certainty="false" faims_read_only="false" ref="Walker">
              <label>Number of Walkers</label>
            </input>
          </group>
        </group>      
        <trigger ref="newSurveyUnit">
          <label>New {Survey_Unit}</label>
        </trigger>
        <select1 appearance="compact" ref="currentSurveyUnit" faims_annotation="false" faims_certainty="false">
            <label>Current {Survey_Unit} Indicator</label>
            <item>
              <label>placeholder</label>
              <value>placeholder</value>
            </item>
        </select1>
        <trigger ref="finishSU">
          <label>Finish {Survey_Unit}</label>
        </trigger>
        <group faims_style="orientation" ref="container2">
          <label/>
          <group faims_style="even" ref="child1">
            <label/>
            <trigger ref="newPOI">
              <label>New {POI}</label>
            </trigger>
          </group>
          <group faims_style="even" ref="child2">
            <label/>
            <trigger ref="newArtefact">
              <label>New {Artefact}</label>
            </trigger>
          </group>
        </group>
        <input faims_certainty="false" faims_read_only="false" ref="listOfLastText">
          <label>How many records would you like to see?</label>
        </input>
        <select1 ref="archentsRadio" faims_annotation="false" faims_certainty="false">
            <label/>
            <item>
              <label>{Survey_Unit}</label>
              <value>Survey Unit</value>
            </item>
            <item>
              <label>{POI}</label>
              <value>POI</value>
            </item>
            <item>
              <label>{Artefact}</label>
              <value>Artefact</value>
            </item>
        </select1>
        <select1 appearance="compact" ref="listOfLast" faims_annotation="false" faims_certainty="false">
            <label/>
            <item>
              <label>placeholder</label>
              <value>placeholder</value>
            </item>
        </select1>
      </group>

      <group ref="Map" faims_scrollable="false">
        <label>Map</label>
         <input ref="map" faims_map="true" faims_certainty="false">
          <label/>
        </input>
        <trigger ref="centre">
          <label>Centre Me</label>
        </trigger>
      </group>
    </group>
    <group ref="Survey_Unit" faims_archent_type="Survey Unit">
      <label>{Survey_Unit}</label>
      <group ref="Survey_Unit">
        <label>{Survey_Unit}</label>
        <input ref="Survey_Unit_ID" faims_attribute_name="Survey Unit ID" faims_attribute_type="freetext" faims_certainty="false" faims_annotation="false" faims_read_only="false">
          <label>{Survey_Unit_ID}</label>
        </input>
        <input ref="Total_Transect_Width" faims_attribute_name="Total Transect Width" faims_attribute_type="measure" faims_certainty="true" faims_annotation="true" faims_read_only="false">
          <label>Total Transect Width</label>
        </input>
        <input ref="Photo_file_numbers" faims_attribute_name="Photo file numbers" faims_attribute_type="freetext" faims_certainty="true" faims_annotation="false" faims_read_only="false">
          <label>{Photo_file_numbers}</label>
        </input>
        <select1 ref="Landform" faims_attribute_name="Landform" faims_attribute_type="vocab" faims_certainty="true" faims_annotation="true" faims_read_only="false">
          <label>{Landform}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <input ref="Remarks_on_landform" faims_attribute_name="Remarks on landform" faims_attribute_type="freetext" faims_certainty="true" faims_annotation="false" faims_read_only="false">
          <label>{Remarks_on_landform}</label>
        </input>
        <select1 ref="Average_percentage_of_visibility_of_surface_of_the_landform_area" faims_attribute_name="Average percentage of visibility of surface of the landform area" faims_attribute_type="vocab" faims_certainty="true" faims_annotation="true" faims_read_only="false">
          <label>{Average_percentage_of_visibility_of_surface_of_the_landform_area}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <input ref="Remarks_on_visibility" faims_attribute_name="Remarks on visibility" faims_attribute_type="freetext" faims_certainty="true" faims_annotation="false" faims_read_only="false">
          <label>{Remarks_on_visibility}</label>
        </input>
        <select1 ref="Average_percentage_of_exposure_of_artefacts" faims_attribute_name="Average percentage of exposure of artefacts" faims_attribute_type="vocab" faims_certainty="true" faims_annotation="true" faims_read_only="false">
          <label>{Average_percentage_of_exposure_of_artefacts}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <input ref="Remarks_on_exposure" faims_attribute_name="Remarks on exposure" faims_attribute_type="freetext" faims_certainty="true" faims_annotation="false" faims_read_only="false">
          <label>{Remarks_on_exposure}</label>
        </input>
        <select1 ref="Basic_geomorphic_summary" faims_attribute_name="Basic geomorphic summary" faims_attribute_type="vocab" faims_certainty="true" faims_annotation="true" faims_read_only="false">
          <label>{Basic_geomorphic_summary}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <select1 ref="Cobble_distribution" faims_attribute_name="Cobble distribution" faims_attribute_type="vocab" faims_certainty="true" faims_annotation="true" faims_read_only="false">
          <label>{Cobble_distribution}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <input ref="Estimated_number_of_cobbles_per_m2" faims_attribute_name="Estimated number of cobbles per m2" faims_attribute_type="measure" faims_certainty="true" faims_annotation="true" faims_read_only="false">
          <label>{Estimated_number_of_cobbles_per_m2}</label>
        </input>
        <input ref="Other_remarks_on_cobbles" faims_attribute_name="Other remarks on cobbles" faims_attribute_type="freetext" faims_certainty="true" faims_annotation="false" faims_read_only="false">
          <label>{Other_remarks_on_cobbles}</label>
        </input>
        <select1 appearance="full" ref="Sediment_size" faims_attribute_name="Sediment size" faims_attribute_type="vocab" faims_certainty="true" faims_annotation="true" faims_read_only="false">
          <label>{Sediment_size}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <input ref="Other_remarks_on_transect" faims_attribute_name="Other remarks on transect" faims_attribute_type="freetext" faims_certainty="true" faims_annotation="false" faims_read_only="false">
          <label>{Other_remarks_on_transect}</label>
        </input>
        <select type="camera" faims_sync="true" ref="Photo" faims_attribute_name="Transect Photo" faims_attribute_type="freetext" faims_certainty="false" faims_annotation="false" faims_read_only="false">
          <label>{Photos}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select>
        <trigger ref="Attach_Photo">
          <label>{Attach_Photo}</label>
        </trigger>
        <trigger ref="Update_and_Return">
          <label>{Update_and_Return}</label>
        </trigger>
        <trigger ref="Delete">
          <label>{Delete}</label>
        </trigger>
        <input faims_certainty="false" faims_read_only="true" ref="trackStatus">
          <label>Track Log Status</label>
        </input>
      </group>
    </group>
    <group ref="POI" faims_archent_type="POI">
      <label>{POI}</label>
      <group ref="POI">
        <label>{POI}</label>
        <input ref="POI_ID" faims_attribute_name="POI ID" faims_certainty="false" faims_annotation="false" faims_read_only="false">
          <label>{POI_ID}</label>
        </input>
        <group ref="container1" faims_style="orientation">
          <label></label>
          <group faims_style="even" ref="child1">
            <label></label>
            <input ref="Latitude" faims_attribute_name="Latitude" faims_attribute_type="measure" faims_certainty="true" faims_annotation="true" faims_read_only="false">
              <label>{Latitude}</label>
            </input>
          </group>
          <group faims_style="even" ref="child2">
            <label></label>
            <input ref="Longitude" faims_attribute_name="Longitude" faims_attribute_type="measure" faims_certainty="true" faims_annotation="true" faims_read_only="false">
              <label>{Longitude}</label>
            </input>
          </group>
        </group>
        <trigger ref="Take_GPS">
          <label>{Take_GPS}</label>
        </trigger>
        <select1 appearance="full" ref="Object" faims_attribute_name="Object" faims_attribute_type="measure" faims_certainty="true" faims_annotation="true" faims_read_only="false">
          <label>{Object}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <input ref="Weight" faims_attribute_name="Weight" faims_attribute_type="measure" faims_certainty="true" faims_annotation="true" faims_read_only="false">
          <label>{Weight}</label>
        </input>
        <group ref="container2" faims_style="orientation">
          <label>container2</label>
          <group faims_style="even" ref="child1">
            <label>child1</label>
            <input ref="Maximum_length" faims_attribute_name="Maximum length" faims_attribute_type="measure" faims_certainty="true" faims_annotation="true" faims_read_only="false">
              <label>{Maximum_length}</label>
            </input>
          </group>
          <group faims_style="even" ref="child2">
            <label>child2</label>
            <input ref="Maximum_width" faims_attribute_name="Maximum width" faims_attribute_type="measure" faims_certainty="true" faims_annotation="true" faims_read_only="false">
              <label>{Maximum_width}</label>
            </input>
          </group>
          <group faims_style="even" ref="child3">
            <label>child3</label>
            <input ref="Maximum_thickness" faims_attribute_name="Maximum thickness" faims_attribute_type="measure" faims_certainty="true" faims_annotation="true" faims_read_only="false">
              <label>{Maximum_thickness}</label>
            </input>
          </group>
        </group>
        <select1 ref="Raw_material" faims_attribute_name="Raw material" faims_attribute_type="vocab" faims_certainty="true" faims_annotation="true" faims_read_only="false">
          <label>{Raw_material}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <select1 ref="Crystal_Size" faims_attribute_name="Crystal Size" faims_attribute_type="vocab" faims_certainty="true" faims_annotation="true" faims_read_only="false">
          <label>{Crystal_Size}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <select1 ref="Abundance_of_Flaws" faims_attribute_name="Abundance of Flaws" faims_attribute_type="vocab" faims_certainty="true" faims_annotation="true" faims_read_only="false">
          <label>{Abundance_of_Flaws}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
